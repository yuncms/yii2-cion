{
  "name": "yuncms/yii2-coin",
  "description": "coin",
  "keywords": [
    "coin","yii2","yii2-module"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Xu TL",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yii2-user": "~2.0"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "yuncms\\coin\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "coin",
      "i18n": {
        "class":"yii\\i18n\\PhpMessageSource",
        "basePath":"@yuncms/coin/message"
      },
      "migrationNamespace": "@yuncms/coin/migrations",
      "backend": {

      },
      "frontend": {
        "class": "yuncms\\coin\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
